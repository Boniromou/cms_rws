<% else %>
	<%= bread_crumb("fa fa-bank",t("tree_panel.fund_management"),t("tree_panel.promotional_credit")) %>
<% end %>

<%= javascript_include_tag "print.js" %>

<div id="myTabContent1" class="tab-content">
