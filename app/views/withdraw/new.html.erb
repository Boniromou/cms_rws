<% if @exception_transaction == 'yes'%>
  <%= bread_crumb("fa fa-bank",t("deposit_withdrawal.exception"),[t("deposit_withdrawal.exception"), t("tree_panel.fund_out")]) %>
<% else %>
  <%= bread_crumb("fa fa-bank",t("tree_panel.fund_management"),[t("tree_panel.balance"), t("tree_panel.fund_out")]) %>
<% end %>

<% back_button = close_to_balance %>
<% content = render partial: "withdraw/fund_request_form"%>
<% footer = render "shared/confirm_fund_panel" %>
<%= render partial:"shared/player_info" , :locals => {:show_info => true, :show_balance => true, :back_button => back_button, :content => content, :footer => footer, :show_remain => {:withdraw => true}} %>
<%= render partial: "shared/pop_up_panel" %>

<%= javascript_include_tag "validate.input.js" %>
<%= javascript_include_tag "block.enter.js" %>

